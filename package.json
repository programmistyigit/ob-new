{
  "name": "oblivionlog",
  "version": "3.0.0",
  "description": "Telegram message archiving system with bot, userbot, and automated features",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run"
  },
  "keywords": [
    "telegram",
    "bot",
    "userbot",
    "archive",
    "gramjs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "big-integer": "^1.6.52",
    "dotenv": "^16.6.1",
    "fastify": "^4.29.1",
    "input": "^1.0.1",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.1",
    "p-queue": "^9.0.0",
    "pino": "^9.13.1",
    "pino-pretty": "^11.3.0",
    "telegraf": "^4.16.3",
    "telegram": "^2.26.22",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
